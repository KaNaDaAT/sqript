<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>netcoreapp2.1</TargetFramework>
    <AssemblyName>Qrakhen.Sqript</AssemblyName>
    <RootNamespace>Qrakhen.Sqript</RootNamespace>
    <Version>0.4.1</Version>
    <Authors>Qrakhen</Authors>
    <Product>Sqript</Product>
    <PackageId>Sqript.Runtime</PackageId>
    <Description>QrakhenQrakhenQrakhenQrakhenQrakhenQrakhenQrakhenQrakhen</Description>
    <Copyright>Qrakhen</Copyright>
    <PackageLicenseUrl>Qrakhen</PackageLicenseUrl>
    <PackageProjectUrl>https://www.github.com/qrakhen/sqript</PackageProjectUrl>
    <PackageIconUrl />
    <RepositoryType>Qrakhen</RepositoryType>
    <PackageTags>Qrakhen</PackageTags>
    <PackageReleaseNotes></PackageReleaseNotes>
    <NeutralLanguage>en-CC</NeutralLanguage>
    <ApplicationIcon>favicon.ico</ApplicationIcon>
    <Win32Resource />
    <StartupObject />
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <PlatformTarget>AnyCPU</PlatformTarget>
    <OutputPath>bin\build\</OutputPath>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <Optimize>true</Optimize>
    <Prefer32Bit>false</Prefer32Bit>
    <WarningLevel>4</WarningLevel>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="log\**" />
    <EmbeddedResource Remove="log\**" />
    <None Remove="log\**" />
  </ItemGroup>

  <ItemGroup>
    <Compile Remove="Core\Digesters\Expressionizer.cs" />
    <Compile Remove="Core\Digesters\Segment.cs" />
    <Compile Remove="Core\Logic\Expression.cs" />
    <Compile Remove="Core\Logic\Statement.cs" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Properties\" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="System.Runtime.Loader" Version="4.3.0" />
  </ItemGroup>

</Project>
